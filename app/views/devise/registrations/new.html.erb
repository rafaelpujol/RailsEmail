<strong style="font-size: 30px" class="text-primary">Registro de Correo</strong>
<div class="ibox-content">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>



  <div class="form-group">
    <%= f.label :email, "Correo Electronico" %><br />
    <%= f.email_field :email, autofocus: true ,:class => "LoginUser"%>
  </div>

  <div class="form-group">
    <%= f.label :password, "Contraseña" %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> minimo de caractares)</em>
    <% end %><br />
    <%= f.password_field :password,autocomplete: "off",:class => "LoginPass" %>
  </div>

  <div class="form-group">
    <%= f.label :password_confirmation, "Confirmación de Contraseña" %><br />
    <%= f.password_field :password_confirmation,  autocomplete: "off" , :class => "LoginPass"%>
  </div>

  <div class="actions">
    <%= f.submit "Registrar me!", :class => "btn btn-primary block full-width m-b"  %>
  </div>
<% end %>
</div>
<%= render "devise/shared/links" %>
